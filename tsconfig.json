{
  "extends": "@apify/tsconfig",
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "lib": ["DOM", "ES2022"],
    "strict": true,
    "esModuleInterop": true,
  },
  "include": ["./src/**/*"],
}
